{
  "name": "mern-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for mern stack",
  "main": "index.js",
  "scripts": {
    "start":
      "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "bs":
      "npm run clean && npm run build && npm run build:server && npm run start:prod",
    "build":
      "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:server":
      "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "clean": "rimraf dist",
    "lint": "eslint client server || true",
    "test": "mocha client/utils/testSetup.js \"client/**/*.test.js\" || true"
  },
  "author": "Nigel Reoch",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "express": "4.14.0",
    "mongoose": "4.11.11",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "7.1.0",
    "babel-loader": "7.1.1",
    "babel-polyfill": "6.16.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.16.3",
    "chunk-manifest-webpack-plugin": "1.1.2",
    "css-loader": "0.25.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "3.9.1",
    "eslint-plugin-react": "6.6.0",
    "expect": "1.20.2",
    "extract-text-webpack-plugin": "3.0.2",
    "jsdom": "9.8.3",
    "json-loader": "0.5.4",
    "mocha": "3.1.2",
    "nodemon": "1.11.0",
    "postcss-cssnext": "2.8.0",
    "postcss-loader": "1.1.0",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-devtools": "3.4.1",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "webpack": "3.3.0",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.13.0",
    "webpack-manifest-plugin": "1.1.0",
    "webpack2-externals-plugin": "1.0.0"
  }
}
